{
  "schemaVersion": 1,
  "id": "expanded_weapon_enchanting",
  "version": "${version}",
  "name": "Expanded Weapon Enchanting",
  "description": "This mod allows you to put Impaling, Density, Breach, and Wind Burst on a weapon. It also allows you to mix different weapon enchantment types on the same weapon.",
  "authors": [
    "SmushyTaco"
  ],
  "contact": {
    "homepage": "https://www.smushytaco.com/",
    "issues": "https://www.curseforge.com/minecraft/mc-mods/expanded-weapon-enchanting/issues/",
    "sources": "https://github.com/SmushyTaco/Expanded-Weapon-Enchanting/"
  },
  "license": "Apache-2.0",
  "icon": "assets/expanded_armor_enchanting/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.smushytaco.expanded_weapon_enchanting.ExpandedWeaponEnchanting"
      }
    ]
  },
  "mixins": [
    "expanded_weapon_enchanting.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabricloader}",
    "fabric-api": ">=${fabric_api}",
    "fabric-language-kotlin": ">=${fabric_language_kotlin}",
    "minecraft": ">=${minecraft}",
    "java": ">=${java}",
    "owo": ">=${owo_version}"
  }
}